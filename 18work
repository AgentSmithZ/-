#include <math.h>
#include <iostream>
using namespace std;

int main() {
    float ST, ZP;
    cout<<"ST = "; cin>>ST;
    if(ST>5)
    {
        ZP=130;
        if(ZP=130)
        {
            cout<<ZP;
        }
    }
    else
    {
        if(ST<=15)
        {
            ZP=180;
            if(ZP=180)
            {
                cout<<ZP;
            }
        }
        else
        {
            ZP=180+(ST-15)*10;
            cout<<ZP;
        }
    }

    return 0;
}
